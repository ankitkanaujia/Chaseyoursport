{
  "name": "chaseyoursport",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "prestart": "node -e \"if (process.version !== 'v16.18.0') throw new Error('Please use Node.js v16.18.0')\"",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run chaseyoursport:serve-ssr",
    "serve:ssr": "node dist/chaseyoursport/server/main.js",
    "build:ssr": "ng build && ng run chaseyoursport:server && npm run sentry:sourcemaps",
    "prerender": "ng run chaseyoursport:prerender",
    "lint": "npx ng lint",
    "prettier": "npx prettier --write .",
    "changeset": "changeset",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org sports-social --project chaseyoursport-frontend dist/ && sentry-cli sourcemaps upload --org sports-social --project chaseyoursport-frontend dist/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@axiomhq/axiom-node": "^0.12.0",
    "@nguniversal/express-engine": "^12.1.3",
    "@sentry/angular-ivy": "^7.66.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.15.2",
    "primeflex": "^2.0.0",
    "primeicons": "^6.0.1",
    "primeng": "^12.2.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular-eslint/builder": "^16.1.1",
    "@angular-eslint/eslint-plugin": "^16.1.1",
    "@angular-eslint/eslint-plugin-template": "^16.1.1",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "~12.2.11",
    "@angular/compiler-cli": "~12.2.0",
    "@changesets/cli": "^2.26.2",
    "@nguniversal/builders": "^12.1.3",
    "@sentry/cli": "^2.20.6",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^3.0.2",
    "typescript": "~4.3.5"
  }
}
